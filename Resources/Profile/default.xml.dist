            id: fos_user.user_manager
    firewalls:
        main:
            form_login:
                provider: fos_userbundle]]></config>
            </SymfonySecurity>
            <FOSUserBundle>
                <name>FOSUser</name>
                <namespace>FOS\UserBundle</namespace>
                <config><![CDATA[
fos_user:
    db_driver: orm
    provider_key: fos_userbundle
