<?xml version="1.0" ?>
<config>
    <repositories>
        <!-- Customize this part but don't delete -->
        <installer>
            <symfony>
                <source>github.com/symfony/symfony.git</source>
                <target>vendor/symfony</target>
                <revision type="branch">2.0</revision>
            </symfony>
            <monolog>
                <source>github.com/Seldaek/monolog.git</source>
                <target>vendor/monolog</target>
                <revision type="tag">1.0.1</revision>
            </monolog>
            <swiftmailer>
                <source>github.com/swiftmailer/swiftmailer.git</source>
                <target>vendor/swiftmailer</target>
                <revision type="tag">v4.1.3</revision>
            </swiftmailer>
            <assetic>
                <source>github.com/kriswallsmith/assetic.git</source>
                <target>vendor/assetic</target>
                <revision type="branch">1.0.x</revision>
            </assetic>
            <asseticbundle>
                <source>github.com/symfony/AsseticBundle.git</source>
                <target>vendor/bundles/Symfony/Bundle/AsseticBundle</target>
                <revision type="tag">v1.0.1</revision>
            </asseticbundle>
            <twig>
                <source>github.com/fabpot/Twig.git</source>
                <target>vendor/twig</target>
                <revision type="branch">1.1</revision>
            </twig>
            <twigextensions>
                <source>github.com/fabpot/Twig-extensions.git</source>
                <target>vendor/twig-extensions</target>
                <revision type="branch">master</revision>
            </twigextensions>
            <doctrine>
                <source>github.com/doctrine/doctrine2.git</source>
                <target>vendor/doctrine</target>
                <revision type="branch">2.1.x</revision>
            </doctrine>
            <doctrinedbal>
                <source>github.com/doctrine/dbal.git</source>
                <target>vendor/doctrine-dbal</target>
                <revision type="branch">2.1.x</revision>
            </doctrinedbal>
            <doctrinedatafixtures>
                <source>github.com/doctrine/data-fixtures.git</source>
                <target>vendor/doctrine-data-fixtures</target>
                <revision type="branch">master</revision>
            </doctrinedatafixtures>
            <doctrinedatafixturesbundle>
                <source>github.com/symfony/DoctrineFixturesBundle.git</source>
                <target>vendor/bundles/Symfony/Bundle/DoctrineFixturesBundle</target>
                <revision type="branch">master</revision>
            </doctrinedatafixturesbundle>
            <doctrinemigrations>
                <source>github.com/doctrine/migrations.git</source>
                <target>vendor/doctrine-migrations</target>
                <revision type="branch">master</revision>
            </doctrinemigrations>
            <doctrinemigrationsbundle>
                <source>github.com/symfony/DoctrineMigrationsBundle.git</source>
                <target>vendor/bundles/Symfony/Bundle/DoctrineMigrationsBundle</target>
                <revision type="branch">master</revision>
            </doctrinemigrationsbundle>
            <doctrinecommon>
                <source>github.com/doctrine/common.git</source>
                <target>vendor/doctrine-common</target>
                <revision type="branch">2.1.x</revision>
            </doctrinecommon>
            <doctrinemongodb>
                <source>github.com/doctrine/mongodb.git</source>
                <target>vendor/doctrine-mongodb</target>
                <revision type="branch">master</revision>
            </doctrinemongodb>
            <doctrinemongodbodm>
                <source>github.com/doctrine/mongodb-odm.git</source>
                <target>vendor/doctrine-mongodb-odm</target>
                <revision type="branch">master</revision>
            </doctrinemongodbodm>
            <doctrinemongodbbundle>
                <source>github.com/symfony/DoctrineMongoDBBundle.git</source>
                <target>vendor/bundles/Symfony/Bundle/DoctrineMongoDBBundle</target>
                <revision type="branch">master</revision>
            </doctrinemongodbbundle>
            <propelbundle>
                <source>github.com/propelorm/PropelBundle.git</source>
                <target>vendor/bundles/Propel/PropelBundle</target>
                <revision type="branch">master</revision>
            </propelbundle>
            <phing>
                <source>github.com/Xosofox/phing.git</source>
                <target>vendor/phing</target>
                <revision type="branch">master</revision>
            </phing>
            <propel>
                <source>github.com/propelorm/Propel.git</source>
                <target>vendor/propel</target>
                <revision type="branch">master</revision>
            </propel>
            <mandango>
                <source>github.com/mandango/mandango.git</source>
                <target>vendor/mandango</target>
                <revision type="branch">master</revision>
            </mandango>
            <mondator>
                <source>github.com/mandango/mondator.git</source>
                <target>vendor/mondator</target>
                <revision type="branch">master</revision>
            </mondator>
            <mandangobundle>
                <source>github.com/mandango/MandangoBundle.git</source>
                <target>vendor/bundles/Mandango/MandangoBundle</target>
                <revision type="branch">master</revision>
            </mandangobundle>
            <metadata>
                <source>github.com/schmittjoh/metadata.git</source>
                <target>vendor/metadata</target>
                <revision type="branch">master</revision>
            </metadata>
        </installer>
        
        <!-- Add your custom repository here -->
        <!--
        <user>
            <FOSUserBundle>
                <source>github.com/FriendsOfSymfony/UserBundle.git</source>
                <target>vendor/bundles/FOS/UserBundle</target>
                <revision type="branch">master</revision>
            </FOSUserBundle>
        </user>
        -->
    </repositories>

    <bundles>
        <!-- Customize this part but don't delete -->
        <installer>
            <framework>
                <name>Framework</name>
                <namespace>Symfony\Bundle\FrameworkBundle</namespace>
            </framework>
            <monolog>
                <name>Monolog</name>
                <namespace>Symfony\Bundle\MonologBundle</namespace>
            </monolog>
            <twig>
                <name>Twig</name>
                <namespace>Symfony\Bundle\TwigBundle</namespace>
            </twig>
            <swiftmailer>
                <name>Swiftmailer</name>
                <namespace>Symfony\Bundle\SwiftmailerBundle</namespace>
            </swiftmailer>
            <assetic>
                <name>Assetic</name>
                <namespace>Symfony\Bundle\AsseticBundle</namespace>
            </assetic>
            <doctrine>
                <name>Doctrine</name>
                <namespace>Symfony\Bundle\DoctrineBundle</namespace>
            </doctrine>
            <doctrinemigrations>
                <name>DoctrineMigrations</name>
                <namespace>Symfony\Bundle\DoctrineMigrationsBundle</namespace>
            </doctrinemigrations>
            <doctrinefixtures>
                <name>DoctrineFixtures</name>
                <namespace>Symfony\Bundle\DoctrineFixturesBundle</namespace>
            </doctrinefixtures>
            <propel>
                <name>Propel</name>
                <namespace>Propel\PropelBundle</namespace>
            </propel>
            <doctrinemongodb>
                <name>DoctrineMongoDB</name>
                <namespace>Symfony\Bundle\DoctrineMongoDBBundle</namespace>
            </doctrinemongodb>
            <mandangobundle>
                <name>Mandango</name>
                <namespace>Mandango\MandangoBundle</namespace>
            </mandangobundle>
        </installer>

        <!-- Add your custom bundle here -->
        <!--
