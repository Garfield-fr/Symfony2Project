    </bundles>

    <namespaces>
        <!-- Customize this part but don't delete -->
        <installer>
            <symfony>
                <name>Symfony</name>
                <path>vendor/symfony/src</path>
                <path>vendor/bundles</path>
            </symfony>
            <doctrinedatafixtures>
                <name>Doctrine\Common\DataFixtures</name>
                <path>vendor/doctrine-data-fixtures/lib</path>
            </doctrinedatafixtures>
            <doctrinecommon>
                <name>Doctrine\Common</name>
                <path>vendor/doctrine-common/lib</path>
            </doctrinecommon>
            <doctrinemigrations>
                <name>Doctrine\DBAL\Migrations</name>
                <path>vendor/doctrine-migrations/lib</path>
            </doctrinemigrations>
            <doctrinemongodb>
                <name>Doctrine\MongoDB</name>
                <path>vendor/doctrine-mongodb/lib</path>
            </doctrinemongodb>
            <doctrinemongodbodm>
                <name>Doctrine\ODM\MongoDB</name>
                <path>vendor/doctrine-mongodb-odm/lib</path>
            </doctrinemongodbodm>
            <doctrinedbal>
                <name>Doctrine\DBAL</name>
                <path>vendor/doctrine-dbal/lib</path>
            </doctrinedbal>
            <doctrine>
                <name>Doctrine</name>
                <path>vendor/doctrine/lib</path>
            </doctrine>
            <propel>
                <name>Propel</name>
                <path>vendor/bundles</path>
            </propel>
            <assetic>
                <name>Assetic</name>
                <path>vendor/assetic/src</path>
            </assetic>
            <metadata>
                <name>Metadata</name>
                <path>vendor/metadata/src</path>
            </metadata>
            <monolog>
                <name>Monolog</name>
                <path>vendor/monolog/src</path>
            </monolog>
            <mandangobundle>
                <name>Mandango\MandangoBundle</name>
                <path>vendor/bundles</path>
            </mandangobundle>
            <mondator>
                <name>Mondator</name>
                <path>vendor/mondator/src</path>
            </mondator>
            <mandango>
                <name>Mandango</name>
                <path>vendor/mondator/src</path>
            </mandango>
            <mandangomodel>
                <name>Model</name>
                <path>src</path>
            </mandangomodel>
        </installer>
        
        <!-- Add your custom namespace here -->
        <!--
        <user>
            <FOSUserBundle>
                <name>FOS</name>
                <path>vendor/bundles</path>
            </FOSUserBundle>
        </user>
        -->
    </namespaces>

    <!-- Customize this part but don't delete -->
    <prefixes>
        <installer>
            <twigextensions>
                <name>Twig_Extensions</name>
                <path>vendor/twig-extensions/lib</path>
            </twigextensions>
            <twig>
                <name>Twig</name>
                <path>vendor/twig/lib</path>
            </twig>
        </installer>
        
        <!-- Add your custom prefix here -->
        <!--
        <user>
            <...>
                <name>...</name>
                <path>...</path>
            </...>
        </user>
        -->
    </prefixes>
</config>
